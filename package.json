{
  "name": "@codecb/ts-utils",
  "version": "0.3.1",
  "license": "MIT",
  "author": {
    "email": "icodemyownlife@gmail.com",
    "name": "ICodeMyOwnLife"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/code-cb/ts-utils.git"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/cjs/common.cjs",
  "module": "./dist/esm/common.mjs",
  "types": "./dist/esm/common.d.ts",
  "exports": {
    "./boolean": {
      "import": "./dist/esm/boolean.mjs",
      "require": "./dist/cjs/boolean.cjs"
    },
    "./class": {
      "import": "./dist/esm/class.mjs",
      "require": "./dist/cjs/class.cjs"
    },
    "./common": {
      "import": "./dist/esm/common.mjs",
      "require": "./dist/cjs/common.cjs"
    },
    "./function": {
      "import": "./dist/esm/function.mjs",
      "require": "./dist/cjs/function.cjs"
    },
    "./list": {
      "import": "./dist/esm/list.mjs",
      "require": "./dist/cjs/list.cjs"
    },
    "./number": {
      "import": "./dist/esm/number.mjs",
      "require": "./dist/cjs/number.cjs"
    },
    "./object": {
      "import": "./dist/esm/object.mjs",
      "require": "./dist/cjs/object.cjs"
    },
    "./string": {
      "import": "./dist/esm/string.mjs",
      "require": "./dist/cjs/string.cjs"
    },
    "./union": {
      "import": "./dist/esm/union.mjs",
      "require": "./dist/cjs/union.cjs"
    }
  },
  "typesVersions": {
    "*": {
      "boolean": [
        "dist/esm/boolean.d.ts"
      ],
      "class": [
        "dist/esm/class.d.ts"
      ],
      "common": [
        "dist/esm/common.d.ts"
      ],
      "function": [
        "dist/esm/function.d.ts"
      ],
      "list": [
        "dist/esm/list.d.ts"
      ],
      "number": [
        "dist/esm/number.d.ts"
      ],
      "object": [
        "dist/esm/object.d.ts"
      ],
      "string": [
        "dist/esm/string.d.ts"
      ],
      "union": [
        "dist/esm/union.d.ts"
      ]
    }
  },
  "scripts": {
    "build": "rm -rf dist && rollup -c rollup.config.ts --configPlugin rollup-plugin-ts",
    "format": "prettier --write '**/*.ts'",
    "prepublishOnly": "yarn build",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@codecb/prettierrc": "0.0.1",
    "@codecb/tsconfigs": "0.0.9",
    "@types/node": "18.7.17",
    "fast-glob": "3.2.12",
    "prettier": "2.7.1",
    "rollup": "2.79.0",
    "rollup-plugin-terser": "7.0.2",
    "rollup-plugin-ts": "3.0.2",
    "typescript": "4.8.3"
  },
  "prettier": "@codecb/prettierrc"
}
